{
    "name": "lovely-docs-mcp",
    "version": "0.0.0",
    "description": "MCP server for lovely-docs",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./doc-cache": {
            "types": "./dist/lib/doc-cache.d.ts",
            "default": "./dist/lib/doc-cache.js"
        }
    },
    "bin": {
        "lovely-docs-mcp": "dist/index.js"
    },
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "bun run build",
        "publish": "npm publish"
    },
    "keywords": [
        "mcp",
        "mcp-server"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^0.5.0",
        "@types/debug": "^4.1.12",
        "debug": "^4.4.3",
        "valibot": "^1.1.0"
    },
    "devDependencies": {
        "@types/node": "^20.19.24",
        "typescript": "^5.9.3"
    }
}